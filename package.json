    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "express-session": "^1.15.6",
    "helmet": "^3.8.1",
    "http": "^0.0.0",
